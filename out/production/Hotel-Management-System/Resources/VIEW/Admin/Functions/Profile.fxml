<?import javafx.scene.text.Font?>


<Pane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Admin.Functions.ProfileController">
   <children>
      <HBox prefHeight="574.0" prefWidth="733.0">
         <children>
            <Pane prefHeight="574.0" prefWidth="356.0">
               <children>
                  <Label contentDisplay="CENTER" prefHeight="72.0" prefWidth="358.0" text="GENERALE INFORMATION" textFill="#909090">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                     <padding>
                        <Insets left="30.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="First_name" layoutX="19.0" layoutY="94.0" prefHeight="43.0" prefWidth="319.0" promptText="FIRST NAME">
                     <padding>
                        <Insets left="20.0" />
                     </padding>
                  </TextField>
                  <TextField fx:id="Last_name" layoutX="19.0" layoutY="155.0" prefHeight="43.0" prefWidth="319.0" promptText="LAST NAME">
                     <padding>
                        <Insets left="20.0" />
                     </padding>
                  </TextField>
                  <TextField fx:id="Email" layoutX="19.0" layoutY="215.0" prefHeight="43.0" prefWidth="319.0" promptText="EMAIL">
                     <padding>
                        <Insets left="20.0" />
                     </padding>
                  </TextField>
                  <TextField fx:id="Phone" layoutX="19.0" layoutY="394.0" prefHeight="43.0" prefWidth="319.0" promptText="PHONE">
                     <padding>
                        <Insets left="20.0" />
                     </padding>
                  </TextField>
                  <Button layoutX="64.0" layoutY="477.0" mnemonicParsing="false" onAction="#UpdateProfile" prefHeight="42.0" prefWidth="233.0" styleClass="Admin_profile" stylesheets="@../../../CSS/Admin.css" text="SEND VALIDATION" />
                  <Label fx:id="Message" layoutX="19.0" layoutY="60.0" prefHeight="22.0" prefWidth="319.0" />
                  <PasswordField fx:id="Password" layoutX="19.0" layoutY="276.0" prefHeight="44.0" prefWidth="318.0" promptText="PASSWORD">
                     <padding>
                        <Insets left="20.0" />
                     </padding>
                  </PasswordField>
                  <PasswordField fx:id="Con_password" layoutX="19.0" layoutY="333.0" prefHeight="44.0" prefWidth="318.0" promptText="CONFIRME PASSWORD">
                     <padding>
                        <Insets left="20.0" />
                     </padding>
                  </PasswordField>
               </children>
            </Pane>
            <VBox prefHeight="449.0" prefWidth="354.0">
               <children>
                  <ImageView cache="true" cacheHint="QUALITY" fitHeight="385.0" fitWidth="352.0" layoutY="100.0" pickOnBounds="true" smooth="false">
                     <image>
                        <Image url="@../../../IMAGES/employer.png" />
                     </image>
                     <VBox.margin>
                        <Insets top="80.0" />
                     </VBox.margin>
                  </ImageView>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</Pane>
